    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "pygrep",
            "args": [
                "-p", "\\w+\\s+DST=(123.12.123.12)\\s+\\w+", "1",
                "-f", "ufw.test"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python: 002: start end omitall",
            "type": "python",
            "request": "launch",
            "module": "pygrep",
            "args": [
                "-s", "SRC=", "1",
                "-e", " DST=", "1",
                "-O",
                "-f", "ufw.test"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}